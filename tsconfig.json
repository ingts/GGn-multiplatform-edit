{
  "compilerOptions": {
    "strict": true,
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "target": "ES2020",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "jsxImportSource": "@emotion/react",
    "module": "es6",
    "typeRoots": ["./types", "./node_modules/@types", "./typings.d.ts"]
  },
  "exclude": ["node_modules"],
  "include": ["./src/"]
}
